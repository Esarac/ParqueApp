            <Button mnemonicParsing="false" onAction="#back" prefHeight="23.0" prefWidth="100.0" text="Back">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="pay" mnemonicParsing="false" onAction="#pay" prefHeight="25.0" prefWidth="100.0" text="Pay">
