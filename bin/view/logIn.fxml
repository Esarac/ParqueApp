<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="pane" alignment="CENTER" fillWidth="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="15.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControlLogIn">
   <children>
      <ImageView id="logo" fx:id="twiceLogo" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" smooth="false">
         <VBox.margin>
            <Insets right="20.0" />
         </VBox.margin>
      </ImageView>
      <TextField fx:id="email" focusTraversable="false" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="251.0" promptText="Email">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font></TextField>
      <PasswordField fx:id="password" focusTraversable="false" minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="252.0" promptText="Password">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </PasswordField>
      <CheckBox fx:id="logging" focusTraversable="false" mnemonicParsing="false" text="Keep logging">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </CheckBox>
      <Button fx:id="logIn" mnemonicParsing="false" onAction="#logIn" prefHeight="25.0" prefWidth="82.0" text="Log In">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="register" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#register" prefHeight="28.0" prefWidth="87.0" style="-fx-background-color: transparent;" text="Register">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
   </padding>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
